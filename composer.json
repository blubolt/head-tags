{
    "name": "blubolt/head-tags",
    "description": "Library for building head tags with a focus on SEO.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandr Vysotsky",
            "email": "utlime@gmail.com"
        },
        {
            "name": "Trevor North",
            "email": "trevor@blubolt.com"
        }
    ],
    "keywords": ["seo", "social", "meta", "og", "open graph", "twitter card"],
    "autoload" : {
        "psr-4" : {"blubolt\\HeadTags\\" : "src/"}
    },
    "autoload-dev" : {
        "psr-4" : {"blubolt\\HeadTags\\Tests\\" : "tests/"}
    },
    "require": {
        "php": "^7.2",
        "ext-dom": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-phpunit": "^0.11.0",
        "ext-libxml": "*",
        "squizlabs/php_codesniffer": "^3.4",
        "slevomat/coding-standard": "^5.0"
    },
    "scripts": {
        "test": "phpunit --colors=always --testdox --coverage-text",
        "phpcs": "phpcs -s",
        "phpstan": "phpstan analyse"
    }
}
